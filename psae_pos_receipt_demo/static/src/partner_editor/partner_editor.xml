<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="psae_pos_receipt_demo.PartnerDetailsEdit" t-inherit="point_of_sale.PartnerDetailsEdit" t-inherit-mode="extension">
       <xpath expr="//div[hasclass('partner-details-box')]" position="inside">
           <div class="partner-detail col">
               <label class="form-label label">Arabic Name</label>
                <input
                       class="detail form-control"
                       name="arabic_name"
                       t-model="changes.arabic_name"
                />
           </div>
        </xpath>
    </t>
</templates>
